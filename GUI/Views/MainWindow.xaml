        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="601">
    <Grid>
        <TabControl x:Name="tabControl">
            <TabItem x:Name="LogsViewTab"  Header="Logs">
